                    </graphic>
                    <styleClass>
                        <String fx:value="button"/>
                    </styleClass>
                </Button>
                <Button fx:id="btnRelatorios" mnemonicParsing="false" prefHeight="50.0" prefWidth="130.0" style=" -fx-text-fill: #67c2ff; -fx-border-radius: 15; -fx-background-radius: 15;">
                    <graphic>
                        <ImageView fitHeight="30" fitWidth="30">
                            <Image url="@Images/profit-report.png"/>
                        </ImageView>
                    </graphic>
                    <styleClass>
                        <String fx:value="button"/>
                    </styleClass>
                </Button>
                <Button fx:id="btnSair" mnemonicParsing="false" prefHeight="50.0" prefWidth="130.0" style=" -fx-text-fill: #67c2ff; -fx-border-radius: 15; -fx-background-radius: 15;">
                    <graphic>
                        <ImageView fitHeight="30" fitWidth="30">
                            <Image url="@Images/exit.png"/>
                        </ImageView>
                    </graphic>
                    <styleClass>
                        <String fx:value="button"/>
                    </styleClass>
                </Button>

            </children>
        </VBox>
    </left>
    <center>
        <AnchorPane prefHeight="400.0" prefWidth="600.0">
        </AnchorPane>
    </center>
</BorderPane>





